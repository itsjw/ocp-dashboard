{
  "name": "angular-electron",
  "version": "2.1.1",
  "description": "Angular 5 with Electron (Typescript + SASS + Hot Reload)",
  "homepage": "https://github.com/maximegris/angular-electron",
  "author": {
    "name": "Maxime GRIS",
    "email": "maxime.gris@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 5",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "npm-run-all --parallel webpack:watch electron:serve",
    "webpack:watch": "webpack --watch",
    "start:web": "webpack-dev-server --content-base . --port 4200 --inline",
    "build:electron:mainOLD": "tsc main.ts --outDir dist && copyfiles package.json dist && cd dist && npm install --prod && cd ..",
    "build:electron:main": "tsc main.ts --outDir dist && copyfiles package.json dist && cd dist && npm install --target=1.7.8 --arch=x64 --target_arch=x64 --disturl=https://atom.io/download/electron --runtime=electron --build-from-source=true --prod && cd ..",
    "fix:nfc": "dist/node_modules/.bin/electron-rebuild",
    "rebuild:nfc": "rm -rf dist && tsc main.ts --outDir dist && copyfiles package.json dist && cd dist && npm install --target=1.7.8 --arch=x64 --target_arch=x64 --disturl=https://atom.io/download/electron --runtime=electron --build-from-source=true --prod && cd ..",
    "buildmain": "tsc main.ts --outDir dist && copyfiles package.json dist && cd dist && npm install --target=1.7.5 --arch=x64 --target_arch=x64 --disturl=https://atom.io/download/electron --runtime=electron --build-from-source=true --prod && cd ..",
    "build": "webpack --display-error-details && npm run build:electron:main",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "electron:serve": "npm run build:electron:main && electron ./dist --serve",
    "electron:test": "electron ./dist",
    "electron:dev": "npm run build && electron ./dist",
    "electron:prod": "npm run build:prod && electron ./dist",
    "electron:linux": "npm run build:prod && node package.js --asar --platform=linux --arch=x64",
    "electron:windows": "npm run build:prod && node package.js --asar --platform=win32 --arch=ia32",
    "electron:windows64": "npm run build:prod && node package.js --asar --platform=win32 --arch=x64",
    "electron:winportable": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && node package.js --asar --platform=darwin --arch=x64",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet && npm run build",
    "e2e": "protractor ./protractor.conf.js"
  },
  "dependencies": {
    "@agm/core": "^1.0.0-beta.2",
    "@angular/animations": "^5.0.3",
    "@angular/common": "5.0.2",
    "@angular/compiler": "5.0.2",
    "@angular/core": "5.0.2",
    "@angular/forms": "5.0.2",
    "@angular/http": "5.0.2",
    "@angular/platform-browser": "5.0.2",
    "@angular/platform-browser-dynamic": "5.0.2",
    "@angular/router": "5.0.2",
    "@asymmetrik/angular2-leaflet": "^2.2.1",
    "@nebular/auth": "^2.0.0-rc.3",
    "@nebular/theme": "^2.0.0-rc.3",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "@swimlane/ngx-charts": "^7.0.1",
    "@taptrack/ndef": "^1.0.3",
    "@types/leaflet": "^1.2.2",
    "angular2-chartjs": "^0.4.1",
    "angular2-toaster": "^4.0.1",
    "bootstrap": "^4.0.0-beta.2",
    "buffer": "^5.0.8",
    "ckeditor": "^4.7.3",
    "core-js": "2.4.1",
    "crypto-js": "^3.1.9-1",
    "electron-debug": "^1.4.0",
    "enhanced-resolve": "3.3.0",
    "font-awesome": "^4.7.0",
    "ionicons": "^2.0.1",
    "leaflet": "^1.2.0",
    "ndef-parser": "^1.0.7",
    "nebular-icons": "^1.0.6",
    "nfc-pcsc": "^0.6.1",
    "nfccard-tool": "^0.0.8",
    "ng2-ckeditor": "^1.1.9",
    "ng2-smart-table": "^1.2.2",
    "ng2-tree": "^2.0.0-rc.4",
    "ngx-echarts": "^2.0.0-beta.0",
    "pace-js": "^1.0.2",
    "printer": "^0.2.2",
    "roboto-fontface": "^0.8.0",
    "rxjs": "5.5.2",
    "socicon": "^3.0.5",
    "stacktrace-js": "^2.0.0",
    "tinymce": "^4.7.3",
    "typeface-exo": "0.0.44",
    "zone.js": "0.8.17"
  },
  "devDependencies": {
